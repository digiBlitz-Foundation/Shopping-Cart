<export-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="http://www.yes-cart.org/schema/export-descriptor.xsd">

    <entity-type>org.yes.cart.domain.entity.Address</entity-type>

    <export-file-descriptor>
        <file-encoding>UTF-8</file-encoding>
        <file-name>customeraddresses-{timestamp}.csv</file-name>
        <print-header>true</print-header>
        <column-delimiter>;</column-delimiter>
        <text-qualifier>&quot;</text-qualifier>
    </export-file-descriptor>

    <select-sql>select a from AddressEntity a where a.customer is not null</select-sql>

    <export-columns>
        
        <column-descriptor>
            <column-header>GUID</column-header>
            <field-type>FIELD</field-type>
            <name>guid</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Country Code</column-header>
            <field-type>FIELD</field-type>
            <name>countryCode</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>State Code</column-header>
            <field-type>FIELD</field-type>
            <name>stateCode</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>City</column-header>
            <field-type>FIELD</field-type>
            <name>city</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Address 1</column-header>
            <field-type>FIELD</field-type>
            <name>addrline1</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Address 2</column-header>
            <field-type>FIELD</field-type>
            <name>addrline2</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Post Code</column-header>
            <field-type>FIELD</field-type>
            <name>postcode</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Phone 1</column-header>
            <field-type>FIELD</field-type>
            <name>phone1</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Phone 2</column-header>
            <field-type>FIELD</field-type>
            <name>phone2</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Mobile 1</column-header>
            <field-type>FIELD</field-type>
            <name>mobile1</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Mobile 2</column-header>
            <field-type>FIELD</field-type>
            <name>mobile2</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>First name</column-header>
            <field-type>FIELD</field-type>
            <name>firstname</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Last name</column-header>
            <field-type>FIELD</field-type>
            <name>lastname</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Middle name</column-header>
            <field-type>FIELD</field-type>
            <name>middlename</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Email 1</column-header>
            <field-type>FIELD</field-type>
            <name>email1</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Email 2</column-header>
            <field-type>FIELD</field-type>
            <name>email2</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 0</column-header>
            <field-type>FIELD</field-type>
            <name>custom0</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 1</column-header>
            <field-type>FIELD</field-type>
            <name>custom1</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 2</column-header>
            <field-type>FIELD</field-type>
            <name>custom2</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 3</column-header>
            <field-type>FIELD</field-type>
            <name>custom3</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 4</column-header>
            <field-type>FIELD</field-type>
            <name>custom4</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 5</column-header>
            <field-type>FIELD</field-type>
            <name>custom5</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 6</column-header>
            <field-type>FIELD</field-type>
            <name>custom6</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 7</column-header>
            <field-type>FIELD</field-type>
            <name>custom7</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 8</column-header>
            <field-type>FIELD</field-type>
            <name>custom8</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Custom 9</column-header>
            <field-type>FIELD</field-type>
            <name>custom9</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Is default</column-header>
            <field-type>FIELD</field-type>
            <name>defaultAddress</name>
        </column-descriptor>

        <column-descriptor>
            <column-header>Type</column-header>
            <field-type>FIELD</field-type>
            <name>addressType</name>
        </column-descriptor>

    </export-columns>

</export-descriptor>